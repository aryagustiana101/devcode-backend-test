{
  "name": "devcode-backend-test",
  "version": "1.0.0",
  "description": "Back-end API Node.js case Todo-list",
  "author": "M Arya Anggara Gustiana",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/bin/www.js",
    "start:migrate": "npm run migrate && node dist/bin/www.js",
    "dev": "nodemon bin/www.ts",
    "watch": "concurrently \"tsc --watch\" \"nodemon -q dist/bin/www.js\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.34.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "license": "MIT"
}
